{
  "name": "miniprogram-tailwindcss-plugin",
  "version": "1.0.0",
  "description": "a webpack plugin that allow to use tailwindcss in uniapp",
  "author": "johnhom",
  "main": "./dist/index.js",
  "type": "commonjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "release": "release-it",
    "build": "rollup -c --bundleConfigAsCjs",
    "test": "vitest"
  },
  "license": "ISC",
  "keywords": [
    "tailwindcss",
    "miniprogram",
    "webpack",
    "plugin"
  ],
  "homepage": "https://github.com/johnhom1024/tool-cli/tree/main/packages/mrm-preset",
  "repository": "https://github.com/johnhom1024/tool-cli/tree/main/packages/mrm-preset",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@babel/generator": "^7.21.9",
    "@babel/parser": "^7.21.9",
    "@babel/traverse": "^7.21.5",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "postcss": "^8.4.35",
    "postcss-selector-parser": "^6.0.13",
    "rollup": "^3.22.0",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1"
  },
  "devDependencies": {
    "@johnhom/eslint-config-ts": "^0.3.2",
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.5",
    "@types/webpack-sources": "^3.2.0",
    "defu": "^6.1.4",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "release-it": "^15.11.0",
    "rollup-plugin-delete": "^2.0.0",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.5.3",
    "vitest": "^1.3.0",
    "webpack-sources": "^3.2.3"
  }
}
